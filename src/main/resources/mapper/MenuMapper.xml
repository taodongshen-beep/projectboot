<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.projectboot.mapper.MenuMapper">

    <resultMap id="BaseResultMap" type="com.example.projectboot.pojo.Menu">
            <id property="menuId" column="menu_id" jdbcType="INTEGER"/>
            <result property="prentId" column="prent_id" jdbcType="INTEGER"/>
            <result property="menuName" column="menu_name" jdbcType="VARCHAR"/>
            <result property="menuUrl" column="menu_url" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        menu_id,prent_id,menu_name,
        menu_url
    </sql>
</mapper>
